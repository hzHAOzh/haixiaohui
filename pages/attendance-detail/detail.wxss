/* pages/attendance-detail/detail.wxss */
page{
  background: #fafafa;
}
.container{
  width: 100%;
}
.info-list{
  width: 100%;
  padding: 20rpx 30rpx 0 30rpx;
  box-shadow: 0px 0px 12px -2px rgba(0, 0, 0, 0.1);
  background-color: #fff;
}
.info-item{
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.info-item:not(:last-child){
  border-bottom: 1px solid #a1a1a1;
}
.info-item_key{
  color: #e065e0;
  font-size: 30rpx;
}